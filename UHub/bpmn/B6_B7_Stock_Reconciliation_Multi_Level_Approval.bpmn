<?xml version="1.0" encoding="UTF-8"?>
<bpmn2:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bpmn2="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:bioc="http://bpmn.io/schema/bpmn/biocolor/1.0" xmlns:color="http://www.omg.org/spec/BPMN/non-normative/color/1.0" id="sample-diagram" targetNamespace="http://bpmn.io/schema/bpmn" xsi:schemaLocation="http://www.omg.org/spec/BPMN/20100524/MODEL BPMN20.xsd">
  <bpmn2:collaboration id="Collaboration_Stock_Reconciliation">
    <bpmn2:participant id="Participant_Agency" name="Agency (Kho Agency)" processRef="Process_Agency" />
    <bpmn2:participant id="Participant_UHub" name="UHub System" processRef="Process_UHub" />
    <bpmn2:participant id="Participant_BU" name="BU (Business Unit)" processRef="Process_BU" />
    <bpmn2:participant id="Participant_BU_Level2" name="BU Level-2 (Email Approval)" processRef="Process_BU_Level2" />
    <bpmn2:participant id="Participant_UtopAdmin" name="Utop Admin" processRef="Process_UtopAdmin" />
    <bpmn2:participant id="Participant_UGMS" name="UGMS System" processRef="Process_UGMS" />
    <bpmn2:messageFlow id="MessageFlow_DigitalConfirm" name="Digital Confirm Report" sourceRef="Task_Agency_DigitalConfirm" targetRef="Task_UHub_ReceiveConfirm" />
    <bpmn2:messageFlow id="MessageFlow_SubmitTicket" name="Submit Ticket Đối Soát" sourceRef="Task_Agency_SubmitTicket" targetRef="Task_UHub_ReceiveTicket" />
    <bpmn2:messageFlow id="MessageFlow_EmailNotification" name="Auto Email Ticket Notification" sourceRef="Task_UHub_SendEmailToBU" targetRef="Task_BU_ReceiveTicket" />
    <bpmn2:messageFlow id="MessageFlow_ForwardTicket" name="Forward Ticket for Level-2 Approval" sourceRef="Task_BU_ForwardTicket" targetRef="Task_BU_Level2_ReviewTicket" />
    <bpmn2:messageFlow id="MessageFlow_ApprovalResult" name="Approval/Rejection via Email" sourceRef="Task_BU_Level2_SendDecision" targetRef="Task_BU_ReceiveDecision" />
    <bpmn2:messageFlow id="MessageFlow_UtopConfirm" name="Utop Admin Confirm Ticket" sourceRef="Task_UtopAdmin_ConfirmTicket" targetRef="Task_UHub_ReceiveUtopConfirm" />
    <bpmn2:messageFlow id="MessageFlow_SyncToUGMS_Normal" name="Sync Gift Usage (Normal)" sourceRef="Task_UHub_SyncNormal" targetRef="Task_UGMS_UpdateNormal" />
    <bpmn2:messageFlow id="MessageFlow_SyncToUGMS_Adjustment" name="Sync Adjustment + Evidences" sourceRef="Task_UHub_SyncAdjustment" targetRef="Task_UGMS_UpdateAdjustment" />
    <bpmn2:messageFlow id="MessageFlow_RequestCorrection" name="Request Correction/Re-check" sourceRef="Task_BU_RequestCorrection" targetRef="Task_Agency_Recheck" />
  </bpmn2:collaboration>
  <bpmn2:process id="Process_Agency" name="Agency Process" isExecutable="false">
    <bpmn2:startEvent id="StartEvent_Agency" name="End Campaign&#10;+ 5 Days&#10;(Gift Recall Done)">
      <bpmn2:outgoing>Flow_Agency_1</bpmn2:outgoing>
    </bpmn2:startEvent>
    <bpmn2:task id="Task_Agency_CheckInventory" name="B6_01. Kiểm tra&#10;tổng hàng thu hồi&#10;thực tế vs Report&#10;trên UHub">
      <bpmn2:incoming>Flow_Agency_1</bpmn2:incoming>
      <bpmn2:incoming>Flow_Agency_Recheck</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_2</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_Agency_Recheck" name="Re-check Inventory&#10;(After BU Request)">
      <bpmn2:incoming>Flow_Agency_FromBU</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_Recheck</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_Agency_CheckResult" name="B6_02.&#10;Thu hồi đủ&#10;số còn lại&#10;&#38; không hư?">
      <bpmn2:incoming>Flow_Agency_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_3_Yes</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_Agency_3_No</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_Agency_DigitalConfirm" name="B6_03. Agency&#10;Digital Confirm&#10;Report Đối Soát">
      <bpmn2:incoming>Flow_Agency_3_Yes</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_4</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_Agency_SubmitTicket" name="B6_05. Agency&#10;Submit Ticket&#10;Đối Soát&#10;(Số thực tế + Nguyên nhân)">
      <bpmn2:incoming>Flow_Agency_3_No</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_5</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_Agency_Join">
      <bpmn2:incoming>Flow_Agency_4</bpmn2:incoming>
      <bpmn2:incoming>Flow_Agency_AfterApproval</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_6</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_Agency_StoreRecalled" name="B6_13. Nhập&#10;Gift thu hồi&#10;vào kho Agency&#10;(Ready tái sử dụng)">
      <bpmn2:incoming>Flow_Agency_6</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_7</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_Agency_PrepareReuse" name="B6_14. Quy trình&#10;tái sử dụng Gift">
      <bpmn2:incoming>Flow_Agency_7</bpmn2:incoming>
      <bpmn2:outgoing>Flow_Agency_8</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:endEvent id="EndEvent_Agency" name="Reconciliation&#10;Complete">
      <bpmn2:incoming>Flow_Agency_8</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="Flow_Agency_1" sourceRef="StartEvent_Agency" targetRef="Task_Agency_CheckInventory" />
    <bpmn2:sequenceFlow id="Flow_Agency_2" sourceRef="Task_Agency_CheckInventory" targetRef="Gateway_Agency_CheckResult" />
    <bpmn2:sequenceFlow id="Flow_Agency_3_Yes" name="Yes&#10;(Normal)" sourceRef="Gateway_Agency_CheckResult" targetRef="Task_Agency_DigitalConfirm" />
    <bpmn2:sequenceFlow id="Flow_Agency_3_No" name="No&#10;(Discrepancy)" sourceRef="Gateway_Agency_CheckResult" targetRef="Task_Agency_SubmitTicket" />
    <bpmn2:sequenceFlow id="Flow_Agency_4" sourceRef="Task_Agency_DigitalConfirm" targetRef="Gateway_Agency_Join" />
    <bpmn2:sequenceFlow id="Flow_Agency_5" sourceRef="Task_Agency_SubmitTicket" targetRef="EndEvent_Agency_Wait" />
    <bpmn2:sequenceFlow id="Flow_Agency_6" sourceRef="Gateway_Agency_Join" targetRef="Task_Agency_StoreRecalled" />
    <bpmn2:sequenceFlow id="Flow_Agency_7" sourceRef="Task_Agency_StoreRecalled" targetRef="Task_Agency_PrepareReuse" />
    <bpmn2:sequenceFlow id="Flow_Agency_8" sourceRef="Task_Agency_PrepareReuse" targetRef="EndEvent_Agency" />
    <bpmn2:sequenceFlow id="Flow_Agency_AfterApproval" sourceRef="StartEvent_Agency_AfterApproval" targetRef="Gateway_Agency_Join" />
    <bpmn2:sequenceFlow id="Flow_Agency_FromBU" sourceRef="Task_Agency_Recheck" targetRef="Task_Agency_CheckInventory" />
    <bpmn2:sequenceFlow id="Flow_Agency_Recheck" sourceRef="Task_Agency_Recheck" targetRef="Task_Agency_CheckInventory" />
    <bpmn2:endEvent id="EndEvent_Agency_Wait" name="Wait for&#10;BU Approval">
      <bpmn2:incoming>Flow_Agency_5</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:startEvent id="StartEvent_Agency_AfterApproval" name="After&#10;Ticket Approved">
      <bpmn2:outgoing>Flow_Agency_AfterApproval</bpmn2:outgoing>
    </bpmn2:startEvent>
  </bpmn2:process>
  <bpmn2:process id="Process_UHub" name="UHub System Process" isExecutable="false">
    <bpmn2:laneSet id="LaneSet_UHub">
      <bpmn2:lane id="Lane_UHub_Receiver" name="Data Receiver">
        <bpmn2:flowNodeRef>Task_UHub_ReceiveConfirm</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_ReceiveTicket</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_ReceiveUtopConfirm</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Gateway_UHub_ReconciliationType</bpmn2:flowNodeRef>
      </bpmn2:lane>
      <bpmn2:lane id="Lane_UHub_Processor" name="Data Processor">
        <bpmn2:flowNodeRef>Task_UHub_ValidateConfirm</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_ValidateTicket</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_SendEmailToBU</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_ProcessUtopConfirm</bpmn2:flowNodeRef>
      </bpmn2:lane>
      <bpmn2:lane id="Lane_UHub_Sender" name="Data Sender to UGMS">
        <bpmn2:flowNodeRef>Task_UHub_SyncNormal</bpmn2:flowNodeRef>
        <bpmn2:flowNodeRef>Task_UHub_SyncAdjustment</bpmn2:flowNodeRef>
      </bpmn2:lane>
    </bpmn2:laneSet>
    <bpmn2:task id="Task_UHub_ReceiveConfirm" name="Receive&#10;Digital Confirm&#10;from Agency">
      <bpmn2:outgoing>Flow_UHub_1a</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_ReceiveTicket" name="Receive&#10;Ticket Đối Soát&#10;from Agency">
      <bpmn2:outgoing>Flow_UHub_1b</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_UHub_ReconciliationType">
      <bpmn2:incoming>Flow_UHub_1a</bpmn2:incoming>
      <bpmn2:incoming>Flow_UHub_1b</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UHub_2a</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_UHub_2b</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_UHub_ValidateConfirm" name="B6_04a. Validate&#10;Confirm Data">
      <bpmn2:incoming>Flow_UHub_2a</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UHub_3a</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_SyncNormal" name="B6_04b. Sync&#10;UHub → UGMS&#10;(Gift Usage&#10;Post-Campaign)">
      <bpmn2:incoming>Flow_UHub_3a</bpmn2:incoming>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_ValidateTicket" name="B6_06a. Validate&#10;Ticket Data">
      <bpmn2:incoming>Flow_UHub_2b</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UHub_3b</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_SendEmailToBU" name="B6_06b. Auto Email&#10;Ticket Notification&#10;to BU&#10;(Theo Scheme ID)">
      <bpmn2:incoming>Flow_UHub_3b</bpmn2:incoming>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_ReceiveUtopConfirm" name="Receive&#10;Utop Admin&#10;Confirmation">
      <bpmn2:outgoing>Flow_UHub_4</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_ProcessUtopConfirm" name="Process&#10;Confirmation&#10;&#38; Evidences">
      <bpmn2:incoming>Flow_UHub_4</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UHub_5</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UHub_SyncAdjustment" name="B6_10_02. Sync&#10;UHub → UGMS&#10;(Gift Usage +&#10;Adjustment +&#10;Evidences)">
      <bpmn2:incoming>Flow_UHub_5</bpmn2:incoming>
    </bpmn2:task>
    <bpmn2:sequenceFlow id="Flow_UHub_1a" sourceRef="Task_UHub_ReceiveConfirm" targetRef="Gateway_UHub_ReconciliationType" />
    <bpmn2:sequenceFlow id="Flow_UHub_1b" sourceRef="Task_UHub_ReceiveTicket" targetRef="Gateway_UHub_ReconciliationType" />
    <bpmn2:sequenceFlow id="Flow_UHub_2a" name="Normal" sourceRef="Gateway_UHub_ReconciliationType" targetRef="Task_UHub_ValidateConfirm" />
    <bpmn2:sequenceFlow id="Flow_UHub_2b" name="Discrepancy" sourceRef="Gateway_UHub_ReconciliationType" targetRef="Task_UHub_ValidateTicket" />
    <bpmn2:sequenceFlow id="Flow_UHub_3a" sourceRef="Task_UHub_ValidateConfirm" targetRef="Task_UHub_SyncNormal" />
    <bpmn2:sequenceFlow id="Flow_UHub_3b" sourceRef="Task_UHub_ValidateTicket" targetRef="Task_UHub_SendEmailToBU" />
    <bpmn2:sequenceFlow id="Flow_UHub_4" sourceRef="Task_UHub_ReceiveUtopConfirm" targetRef="Task_UHub_ProcessUtopConfirm" />
    <bpmn2:sequenceFlow id="Flow_UHub_5" sourceRef="Task_UHub_ProcessUtopConfirm" targetRef="Task_UHub_SyncAdjustment" />
  </bpmn2:process>
  <bpmn2:process id="Process_BU" name="BU Process" isExecutable="false">
    <bpmn2:task id="Task_BU_ReceiveTicket" name="Receive Ticket&#10;Notification&#10;via Email">
      <bpmn2:outgoing>Flow_BU_1</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_BU_ReviewTicket" name="B6_07a. BU Review&#10;Ticket Details&#10;&#38; Evidences">
      <bpmn2:incoming>Flow_BU_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_2</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_BU_ForwardTicket" name="B6_07b. BU Raise&#10;&#38; Forward Ticket&#10;via Email&#10;(Get Level-2 Approval)">
      <bpmn2:incoming>Flow_BU_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_3</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:intermediateCatchEvent id="Event_BU_WaitDecision" name="Wait for&#10;Level-2 Decision">
      <bpmn2:incoming>Flow_BU_3</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_4</bpmn2:outgoing>
    </bpmn2:intermediateCatchEvent>
    <bpmn2:task id="Task_BU_ReceiveDecision" name="Receive&#10;Level-2 Decision&#10;via Email">
      <bpmn2:incoming>Flow_BU_4</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_5</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_BU_Decision" name="B6_09.&#10;BU Level-2&#10;Approved?">
      <bpmn2:incoming>Flow_BU_5</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_6_Approved</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_BU_6_Rejected</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_BU_NotifyUtopAdmin" name="Notify&#10;Utop Admin&#10;to Confirm">
      <bpmn2:incoming>Flow_BU_6_Approved</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_7</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:endEvent id="EndEvent_BU_Approved" name="Ticket Approved&#10;→ Wait for&#10;Utop Confirm">
      <bpmn2:incoming>Flow_BU_7</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:task id="Task_BU_RejectTicket" name="B6_11_01. BU&#10;Reject Ticket">
      <bpmn2:incoming>Flow_BU_6_Rejected</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_8</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_BU_RequestCorrection" name="B6_11_02. Request&#10;Agency Correction&#10;/ Re-check">
      <bpmn2:incoming>Flow_BU_8</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_9</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:endEvent id="EndEvent_BU_Loop" name="Loop to&#10;Agency Re-check">
      <bpmn2:incoming>Flow_BU_9</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="Flow_BU_1" sourceRef="Task_BU_ReceiveTicket" targetRef="Task_BU_ReviewTicket" />
    <bpmn2:sequenceFlow id="Flow_BU_2" sourceRef="Task_BU_ReviewTicket" targetRef="Task_BU_ForwardTicket" />
    <bpmn2:sequenceFlow id="Flow_BU_3" sourceRef="Task_BU_ForwardTicket" targetRef="Event_BU_WaitDecision" />
    <bpmn2:sequenceFlow id="Flow_BU_4" sourceRef="Event_BU_WaitDecision" targetRef="Task_BU_ReceiveDecision" />
    <bpmn2:sequenceFlow id="Flow_BU_5" sourceRef="Task_BU_ReceiveDecision" targetRef="Gateway_BU_Decision" />
    <bpmn2:sequenceFlow id="Flow_BU_6_Approved" name="Approved" sourceRef="Gateway_BU_Decision" targetRef="Task_BU_NotifyUtopAdmin" />
    <bpmn2:sequenceFlow id="Flow_BU_6_Rejected" name="Rejected" sourceRef="Gateway_BU_Decision" targetRef="Task_BU_RejectTicket" />
    <bpmn2:sequenceFlow id="Flow_BU_7" sourceRef="Task_BU_NotifyUtopAdmin" targetRef="EndEvent_BU_Approved" />
    <bpmn2:sequenceFlow id="Flow_BU_8" sourceRef="Task_BU_RejectTicket" targetRef="Task_BU_RequestCorrection" />
    <bpmn2:sequenceFlow id="Flow_BU_9" sourceRef="Task_BU_RequestCorrection" targetRef="EndEvent_BU_Loop" />
  </bpmn2:process>
  <bpmn2:process id="Process_BU_Level2" name="BU Level-2 Process" isExecutable="false">
    <bpmn2:task id="Task_BU_Level2_ReviewTicket" name="B6_08. Review&#10;&#38; Approval Ticket&#10;via Email&#10;(With Evidences)">
      <bpmn2:outgoing>Flow_BU_Level2_1</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_BU_Level2_Decision" name="Approve&#10;or&#10;Reject?">
      <bpmn2:incoming>Flow_BU_Level2_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_BU_Level2_2_Approved</bpmn2:outgoing>
      <bpmn2:outgoing>Flow_BU_Level2_2_Rejected</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_BU_Level2_SendDecision" name="Send Decision&#10;via Email&#10;to BU">
      <bpmn2:incoming>Flow_BU_Level2_2_Approved</bpmn2:incoming>
      <bpmn2:incoming>Flow_BU_Level2_2_Rejected</bpmn2:incoming>
    </bpmn2:task>
    <bpmn2:sequenceFlow id="Flow_BU_Level2_1" sourceRef="Task_BU_Level2_ReviewTicket" targetRef="Gateway_BU_Level2_Decision" />
    <bpmn2:sequenceFlow id="Flow_BU_Level2_2_Approved" name="Approved" sourceRef="Gateway_BU_Level2_Decision" targetRef="Task_BU_Level2_SendDecision" />
    <bpmn2:sequenceFlow id="Flow_BU_Level2_2_Rejected" name="Rejected" sourceRef="Gateway_BU_Level2_Decision" targetRef="Task_BU_Level2_SendDecision" />
  </bpmn2:process>
  <bpmn2:process id="Process_UtopAdmin" name="Utop Admin Process" isExecutable="false">
    <bpmn2:startEvent id="StartEvent_UtopAdmin" name="Receive&#10;BU Notification">
      <bpmn2:outgoing>Flow_UtopAdmin_1</bpmn2:outgoing>
    </bpmn2:startEvent>
    <bpmn2:task id="Task_UtopAdmin_ReviewTicket" name="Review&#10;Ticket Details&#10;&#38; Level-2 Approval">
      <bpmn2:incoming>Flow_UtopAdmin_1</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UtopAdmin_2</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UtopAdmin_ConfirmTicket" name="B6_10_01.&#10;Utop Admin&#10;Digital Confirm&#10;Ticket">
      <bpmn2:incoming>Flow_UtopAdmin_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UtopAdmin_3</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:endEvent id="EndEvent_UtopAdmin" name="Ticket Confirmed&#10;→ UHub Sync">
      <bpmn2:incoming>Flow_UtopAdmin_3</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="Flow_UtopAdmin_1" sourceRef="StartEvent_UtopAdmin" targetRef="Task_UtopAdmin_ReviewTicket" />
    <bpmn2:sequenceFlow id="Flow_UtopAdmin_2" sourceRef="Task_UtopAdmin_ReviewTicket" targetRef="Task_UtopAdmin_ConfirmTicket" />
    <bpmn2:sequenceFlow id="Flow_UtopAdmin_3" sourceRef="Task_UtopAdmin_ConfirmTicket" targetRef="EndEvent_UtopAdmin" />
  </bpmn2:process>
  <bpmn2:process id="Process_UGMS" name="UGMS Process" isExecutable="false">
    <bpmn2:task id="Task_UGMS_UpdateNormal" name="Update Stock&#10;(Normal&#10;Reconciliation)">
      <bpmn2:outgoing>Flow_UGMS_1</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:task id="Task_UGMS_UpdateAdjustment" name="Update Stock&#10;(Adjustment +&#10;Evidences)">
      <bpmn2:outgoing>Flow_UGMS_2</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:exclusiveGateway id="Gateway_UGMS_Join">
      <bpmn2:incoming>Flow_UGMS_1</bpmn2:incoming>
      <bpmn2:incoming>Flow_UGMS_2</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UGMS_3</bpmn2:outgoing>
    </bpmn2:exclusiveGateway>
    <bpmn2:task id="Task_UGMS_Log" name="Log Transaction&#10;&#38; Audit Trail">
      <bpmn2:incoming>Flow_UGMS_3</bpmn2:incoming>
      <bpmn2:outgoing>Flow_UGMS_4</bpmn2:outgoing>
    </bpmn2:task>
    <bpmn2:endEvent id="EndEvent_UGMS" name="UGMS&#10;Stock Updated">
      <bpmn2:incoming>Flow_UGMS_4</bpmn2:incoming>
    </bpmn2:endEvent>
    <bpmn2:sequenceFlow id="Flow_UGMS_1" sourceRef="Task_UGMS_UpdateNormal" targetRef="Gateway_UGMS_Join" />
    <bpmn2:sequenceFlow id="Flow_UGMS_2" sourceRef="Task_UGMS_UpdateAdjustment" targetRef="Gateway_UGMS_Join" />
    <bpmn2:sequenceFlow id="Flow_UGMS_3" sourceRef="Gateway_UGMS_Join" targetRef="Task_UGMS_Log" />
    <bpmn2:sequenceFlow id="Flow_UGMS_4" sourceRef="Task_UGMS_Log" targetRef="EndEvent_UGMS" />
